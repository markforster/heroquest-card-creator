{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "heroquest-card-creator",
  "version": "0.1.0",
  "identifier": "info.markforster.heroquest-card-creator",
  "build": {
    "devUrl": null,
    "frontendDist": "../out"
  },
  "app": {
    "windows": [
      {
        "title": "heroquest-card-creator",
        "width": 1065,
        "height": 772,
        "minWidth": 1065,
        "minHeight": 772,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
